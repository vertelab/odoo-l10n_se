<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <data>
            <template id="assets_backend" name="mis_builder" inherit_id="web.assets_backend">
                <xpath expr="." position="inside">
                    <script
                        type="text/javascript"
                        src="/l10n_se_tax_report/static/src/js/mis_report_widget_tax_report.js"
                    />
                </xpath>
            </template>

            <record id="l10n_mis_inherit_account_moms_declaration" model="ir.ui.view">
                  <field name="name">l10n_mis_template</field>
                  <field name="model">account.vat.declaration</field>
                  <field name="inherit_id" ref="l10n_se_tax_report.view_account_moms_declaration"></field>
                    <field name="arch" type="xml">
                    <xpath expr="//field[@name='move_id'][last()]" position="after">
                            <field name="eskd_file_mis"/>
                    </xpath>
<!--                Gammal lösning. Uppdaterar sig inte efter att man har bytt record. Vilket är synd
                    <xpath expr="//notebook" position="inside">
                      <page string="Mis Rader">
                            <field name="generated_mis_report_int" widget="mis_report_widget.tax.report" nolabel="1"/>
                      </page>
                    </xpath>
-->
                    <xpath expr="//header" position="inside">
                        <button name="show_preview" string="Show Preview" type="object" class="oe_highlight" states="done,confirmed,canceled"/>
                    </xpath>

                  </field>
            </record>
     </data>
</odoo>
