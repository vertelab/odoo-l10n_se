    <data>
        <record id="update_account_tax" model="ir.actions.server">
            <field name="name">Uppdatera skatter</field>
            <field name="model_id" ref="base.model_ir_actions_server"/>
            <field name="state">code</field>
            <field name="condition">True</field>
            <field name="code">for template in env['account.tax.template'].search([]):
    account_tax = env['account.tax'].search([('name', '=', template.name)])
    try:
        if not account_tax:
            account_tax = env['account.tax'].create({
                'name': template.name,
                'amount_type': template.amount_type,
                'type_tax_use': template.type_tax_use,
                'tax_group_id': template.tax_group_id.id,
                'account_id': env['account.account'].search([('code', '=', template.account_id.code)]).id,
                'refund_account_id': env['account.account'].search([('code', '=', template.refund_account_id.code)]).id,
                'amount': template.amount,
                'description': template.description,
                'price_include': template.price_include,
            })
            if account_tax.amount_type == 'group':
                children_taxs = template.children_tax_ids.mapped('name')
                if len(children_taxs) > 0:
                    account_tax.write({'children_tax_ids': [(6, 0, env['account.tax'].search([('name', 'in', children_taxs)]).mapped('id'))]})
        else:
            account_tax.write({
                'amount_type': template.amount_type,
                'type_tax_use': template.type_tax_use,
                'tax_group_id': template.tax_group_id.id,
                'account_id': env['account.account'].search([('code', '=', template.account_id.code)]).id,
                'refund_account_id': env['account.account'].search([('code', '=', template.refund_account_id.code)]).id,
                'amount': template.amount,
                'description': template.description,
                'price_include': template.price_include,
            })
            if account_tax.amount_type == 'group':
                children_taxs = template.children_tax_ids.mapped('name')
                if len(children_taxs) > 0:
                    account_tax.write({'children_tax_ids': [(6, 0, env['account.tax'].search([('name', 'in', children_taxs)]).mapped('id'))]})
    except:
        log('Fel att konvertera template: %s till konto: %s' %(template, account_tax))
            </field>
        </record>
