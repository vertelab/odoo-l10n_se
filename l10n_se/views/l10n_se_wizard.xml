<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_wizard_multi_chart_se" model="ir.ui.view">
            <field name="name">Generate Chart of Accounts from a Chart Template</field>
            <field name="model">wizard.multi.charts.accounts</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="account.view_wizard_multi_chart"/>
            <field name="arch" type="xml">
              <data>
                    <xpath expr="//form/group/group/separator" position="attributes">
                        <attribute name="string">
                        Generate Your SE Accounting Chart from a Chart Template
                        </attribute>
                    </xpath>
                    <xpath expr="//form/group/group/label" position="attributes">
                        <attribute name="string">Choose your company type, K1 or normal etc. SELECT 4 DIGIT ACCOUNTS.  This is the same wizard that runs from Financial Management/Configuration/Financial Accounting/ Financial Accounts/Generate Chart of Accounts from a Chart Template.</attribute>
                    </xpath>
              </data>
            </field>
        </record>

        <record id="action_wizard_multi_chart_se" model="ir.actions.act_window">
            <field name="name">Generate Swedish Chart of Accounts from a Chart Template</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.multi.charts.accounts</field>
            <field name="view_id" ref="view_wizard_multi_chart_se"/>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record id="l10n_se_latest_wizard_id" model="ir.actions.todo">
            <field name="action_id" ref="action_wizard_multi_chart_se"/>
            <field name="sequence">10</field>
            <field name="type">once</field>
            <field name="category_id" ref="account.category_accounting_configuration" />
            <field name="groups_id" eval="[(6, 0, [ref('account.group_account_user')])]" />
            <field name="state">open</field>
        </record>

    </data>
</openerp>
